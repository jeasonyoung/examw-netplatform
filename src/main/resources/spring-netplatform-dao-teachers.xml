<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- dao -->
	
	<!-- 主讲教师数据接口 -->
	<bean id="teacherMapper" parent="baseMapper">
	    <!--  主讲教师接口 -->
	    <property name="mapperInterface" value="com.examw.netplatform.dao.admin.teachers.TeacherMapper" />
	</bean>
	
    <!-- 教师答疑主题数据接口 -->
    <bean id="answerQuestionTopicMapper"  parent="baseMapper">
        <!-- 答疑主题接口 -->
        <property name="mapperInterface"  value="com.examw.netplatform.dao.admin.teachers.AnswerQuestionTopicMapper" />
    </bean>
    
    <!-- 教师答疑明细数据接口 -->
    <bean id="answerQuestionDetailMapper" parent="baseMapper">
        <!-- 答疑明细接口 -->
        <property name="mapperInterface"  value="com.examw.netplatform.dao.admin.teachers.AnswerQuestionDetailMapper" />
    </bean>
    
</beans>