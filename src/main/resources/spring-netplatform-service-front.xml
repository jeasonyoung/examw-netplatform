<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- service -->
	<!-- 前台用户服务 -->
	<bean id="frontUserService" class="com.examw.netplatform.service.front.user.impl.FrontUserServiceImpl">
	    <!-- 机构用户数据接口 -->
		<property name="agencyUserDao" ref="agencyUserDao" />
		<!-- 密码工具 -->
		<property name="passwordHelper" ref="passwordHelper" />
	</bean>
	<!-- 前台考试分类接口 -->
	<bean id="frontCategoryService" class="com.examw.netplatform.service.front.user.impl.FrontCategoryServiceImpl">
	    <!-- 考试分类数据接口 -->
		<property name="categoryDao" ref="categoryDao" />
		<!-- 科目服务接口 -->
		<property name="examService" ref="examService" />
	</bean>
</beans>