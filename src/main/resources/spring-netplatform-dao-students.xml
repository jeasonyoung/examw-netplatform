<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- 学员数据接口实现 start -->
	<!-- 订单数据接口 -->
	<bean id="orderMapper" parent="baseMapper">
	    <!-- 订单接口 -->
	    <property name="mapperInterface"  value="com.examw.netplatform.dao.admin.students.OrderMapper" />
	</bean>
	
	<!-- 学习进度数据接口 -->
	<bean id="learningMapper"  parent="baseMapper">
	    <!-- 学习进度接口 -->
	    <property name="mapperInterface"  value="com.examw.netplatform.dao.admin.students.LearningMapper" />
	</bean>
	<!-- 学员数据接口实现 end -->
</beans>